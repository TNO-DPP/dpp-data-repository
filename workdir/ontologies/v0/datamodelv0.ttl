@prefix dpp: <http://example.org/ontology/digitalproductpassport#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix uuid: <urn:uuid:> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .

# Classes
dpp:DigitalProductPassport a rdfs:Class .

# General property
dpp:subComponent a rdf:Property ;
    rdfs:domain dpp:DigitalProductPassport ;
    rdfs:range dpp:DigitalProductPassport .

dpp:eventLog a rdf:Property ;
    rdfs:domain dpp:DigitalProductPassport ;
    rdfs:range prov:Activity .

# General Properties
# dpp:uniqueId a rdf:Property ;
#     rdfs:domain dpp:DigitalProductPassport ;
#     rdfs:range xsd:string .

dpp:ownership a rdf:Property ;
    rdfs:domain dpp:DigitalProductPassport ;
    rdfs:range dpp:OwnershipRecord .

# dpp:eventLog a rdf:Property ;
#     rdfs:domain dpp:DigitalProductPassport ;
#     rdfs:range dpp:EventRecord .

dpp:attribute a rdf:Property ;
    rdfs:domain dpp:DigitalProductPassport ;
    rdfs:range dpp:Attribute .


# Ownership Record
dpp:OwnershipRecord a rdfs:Class .
dpp:hasCurrentOwner a rdf:Property ;
    rdfs:domain dpp:OwnershipRecord ;
    rdfs:range dpp:Owner .

dpp:ownershipTransferLog a rdf:Property ;
    rdfs:domain dpp:OwnershipRecord ;
    rdfs:range dpp:OwnershipTransferEvent .

# Owner Entity
dpp:Owner a rdfs:Class .
dpp:ownerName a rdf:Property ;
    rdfs:domain dpp:Owner ;
    rdfs:range xsd:string .

# Event Record
dpp:EventRecord a rdfs:Class .
dpp:Event a rdfs:Class .
dpp:hasEvent a rdf:Property ;
    rdfs:domain dpp:EventRecord ;
    rdfs:range dpp:Event .

# Ownership Transfer Event
dpp:OwnershipTransferEvent a dpp:Event .
dpp:transferFrom a rdf:Property ;
    rdfs:domain dpp:OwnershipTransferEvent ;
    rdfs:range dpp:Owner .

dpp:transferTo a rdf:Property ;
    rdfs:domain dpp:OwnershipTransferEvent ;
    rdfs:range dpp:Owner .

dpp:transferDate a rdf:Property ;
    rdfs:domain dpp:OwnershipTransferEvent ;
    rdfs:range xsd:dateTime .

# Attributes
dpp:Attribute a rdfs:Class .
dpp:attributeName a rdf:Property ;
    rdfs:domain dpp:Attribute ;
    rdfs:range xsd:string .

dpp:attributeValue a rdf:Property ;
    rdfs:domain dpp:Attribute ;
    rdfs:range xsd:string .