# RenewCycle SolarTech GmbH

@prefix dpp: <http://example.org/ontology/digitalproductpassport#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix prov:  <http://www.w3.org/ns/prov#> .

## Define new Activity Classes for Remanufacturer
dpp:DisassemblyEvent a prov:Activity ;
    prov:wasAssociatedWith dpp:Remanufacturer ;
    prov:qualifiedAssociation a prov:Association ;
        prov:hadPlan dpp:DisassemblyPlan ;
        
dpp:InspectionEvent a prov:Activity ;
    prov:wasAssociatedWith dpp:Remanufacturer ;
    prov:qualifiedAssociation a prov:Association ;
        prov:hadPlan dpp:InspectionPlan ;
        
dpp:RefurbishingEvent a prov:Activity ;
    prov:wasAssociatedWith dpp:Remanufacturer ;
    prov:qualifiedAssociation a prov:Association ;
        prov:hadPlan dpp:RefurbishingPlan ;

dpp:PartSwappingEvent a prov:Activity ;
    prov:wasAssociatedWith dpp:Remanufacturer ;
    prov:qualifiedAssociation a prov:Association ;
        prov:hadPlan dpp:PartSwappingPlan ;

dpp:InformationSalvagingEvent a prov:Activity ;
    prov:wasAssociatedWith dpp:Remanufacturer ;
    prov:qualifiedAssociation a prov:Association ;
        prov:hadPlan dpp:InformationSalvagingPlan ;

dpp:RemanufacturedPanelAssemblyEvent a prov:Activity ;
    prov:wasAssociatedWith dpp:Remanufacturer ;
    prov:qualifiedAssociation a prov:Association ;
        prov:hadPlan dpp:RemanufacturedPanelAssemblyPlan ;

dpp:LogisticsPlaningEvent a prov:Activity ;
    prov:wasAssociatedWith dpp:Remanufacturer ;
    prov:qualifiedAssociation a prov:Association ;
        prov:hadPlan dpp:LogisticsPlaningPlan ;
        
dpp:RemanufacturedPanelShippingEvent a prov:Activity ;
    prov:wasAssociatedWith dpp:Remanufacturer ;
    prov:qualifiedAssociation a prov:Association ;
        prov:hadPlan dpp:RemanufacturedPanelShippingPlan ;

dpp:PackagingEvent a prov:Activity ;
    prov:wasAssociatedWith dpp:Remanufacturer ;
    prov:qualifiedAssociation a prov:Association ;
        prov:hadPlan dpp:PackagingPlan ; 

## Additional properties for solar panel during remanufacturing
dpp:SolarPanel ;

    dpp:remanufacturedLifeExpectancy a rdf:Property ; 
    rdfs:domain dpp:SolarPanel ;
    rdfs:range xsd:int ;
    
    dpp:remainingValue a rdf:Property ; 
    rdfs:domain dpp:SolarPanel ;
    rdfs:range xsd:float ;

    dpp:remanufactureDate a rdf:Property ; 
    rdfs:domain dpp:SolarPanel ;
    rdfs:range xsd:dateTime ;
    
    dpp:replacementParts a rdf:Property ; 
    rdfs:domain dpp:SolarPanel ;
    rdfs:range xsd:string ;
    
    dpp:remanufactureLocation a rdf:Property ;
    rdfs:domain dpp:SolarPanel ;
    rdfs:range xsd:strin ;
    
    dpp:remanufactureProcessDetails a rdf:Property ; 
    rdfs:domain dpp:SolarPanel ;
    rdfs:range xsd:string ; 
    
    dpp:qualityCheckAfterRemanufacture a rdf:Property ; 
    rdfs:domain dpp:SolarPanel ;
    rdfs:range xsd:boolean ;

    dpp:remanufactureCost a rdf:Property ; 
    rdfs:domain dpp:SolarPanel ;
    rdfs:range xsd:float ;
   
    dpp:salvagedInformation a rdf:Property ;
    rdfs:domain dpp:SolarPanel ;
    rdfs:range xsd:strin ;

    dpp:remanufactureValidation a rdf:Property ; 
    rdfs:domain dpp:SolarPanel ;
    rdfs:range xsd:boolean ;
    
    dpp:remanufacturedBy a rdf:Property ; 
    rdfs:domain dpp:SolarPanel ;
    rdfs:range dpp:Remanufacturer  ;
    
    dpp:compactnessPostRemanufacture a rdf:Property ; # After remanufacture the panel may take less space
    rdfs:domain dpp:SolarPanel ;
    rdfs:range xsd:float ; 

    dpp:functionalityPostRemanufacture a rdf:Property ; # Functionality may be improved or adjusted
    rdfs:domain dpp:SolarPanel ;
    rdfs:range xsd:float ;
    
    dpp:warrantyPostRemanufacture a rdf:Property ;  # Remanufacturer may provide additional warranty
    rdfs:domain dpp:SolarPanel ;
    rdfs:range xsd:int .