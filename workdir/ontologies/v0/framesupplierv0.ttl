#  SolarEclipse Panels Ltd.

@prefix dpp: <http://example.org/ontology/digitalproductpassport#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix prov:  <http://www.w3.org/ns/prov#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .

dpp:SolarPanelFrame a dpp:DigitalProductPassport ; 
    dpp:uniqueId a rdf:Property ;
    rdfs:domain dpp:DigitalProductPassport ;
    rdfs:range xsd:string ;

    dpp:ownership a rdf:Property ;
    rdfs:domain dpp:DigitalProductPassport ;
    rdfs:range dpp:OwnershipRecord ;

    dpp:eventLog a rdf:Property ;
    rdfs:domain dpp:DigitalProductPassport ;
    rdfs:range dpp:EventRecord ;

    dpp:attribute a rdf:Property ;
    rdfs:domain dpp:DigitalProductPassport ;
    rdfs:range dpp:Attribute ;

    dpp:subComponent a rdf:Property ;
    rdfs:domain dpp:DigitalProductPassport ;
    rdfs:range dpp:DigitalProductPassport ;

    dpp:mechanicalCharacteristic a dpp:MechanicalCharacteristic ;
    dpp:materialCharacteristic a dpp:MaterialCharacteristic ;
    dpp:environmentalImpact a dpp:EnvironmentalImpact .


## Define new classes for characteristics and impact
dpp:MechanicalCharacteristic a rdfs:Class ;
    dpp:attributeName a rdf:Property ;
    rdfs:domain dpp:Attribute ;
    rdfs:range xsd:string ;

    dpp:attributeValue a rdf:Property ;
    rdfs:domain dpp:Attribute ;
    rdfs:range xsd:string .

dpp:MaterialCharacteristic a rdfs:Class ;
    dpp:attributeName a rdf:Property ;
    rdfs:domain dpp:Attribute ;
    rdfs:range xsd:string ;

    dpp:attributeValue a rdf:Property ;
    rdfs:domain dpp:Attribute ;
    rdfs:range xsd:string .


## Properties for Mechanical Characteristic
dpp:strength a rdf:Property ;
    rdfs:domain dpp:MechanicalCharacteristic ;
    rdfs:range xsd:float ;
    dpp:attribute dpp:Attribute 

dpp:lifeExpectancy a rdf:Property ;
    rdfs:domain dpp:MechanicalCharacteristic ;
    rdfs:range xsd:float ;
    dpp:attribute dpp:Attribute 

dpp:tensileStrength a rdf:Property ;
    rdfs:domain dpp:MechanicalCharacteristic ;
    rdfs:range xsd:float ;
    dpp:attribute dpp:Attribute 


## Properties for Material Characteristic
dpp:materialType a rdf:Property ;
    rdfs:domain dpp:MaterialCharacteristic ;
    rdfs:range xsd:string ;
    dpp:attribute dpp:Attribute 

dpp:materialCoating a rdf:Property ;
    rdfs:domain dpp:MaterialCharacteristic ;
    rdfs:range xsd:string ;
    dpp:attribute dpp:Attribute 


## Activity Classes Specific to Supplier2
dpp:MaterialSelectionEvent a prov:Activity ;
    prov:wasAssociatedWith dpp:Supplier2 ;
    prov:qualifiedAssociation a prov:Association ;
        prov:hadPlan dpp:MaterialSelectionPlan ;

dpp:FrameManufacturingEvent a prov:Activity ;
    prov:wasAssociatedWith dpp:Supplier2 ;
    prov:qualifiedAssociation a prov:Association ;
        prov:hadPlan dpp:FrameManufacturingPlan ;

dpp:CoatingApplicationEvent a prov:Activity ;
    prov:wasAssociatedWith dpp:Supplier2 ;
    prov:qualifiedAssociation a prov:Association ;
        prov:hadPlan dpp:CoatingApplicationPlan ; 

dpp:QualityCheckEvent a prov:Activity ;
    prov:wasAssociatedWith dpp:Supplier2 ;
    prov:qualifiedAssociation a prov:Association ;
        prov:hadRole dpp:QualityInspector ; 
        prov:hadPlan dpp:QualityCheckPlan ;

dpp:PackingEvent a prov:Activity ;
    prov:wasAssociatedWith dpp:Supplier2 ;
    prov:qualifiedAssociation a prov:Association ;
        prov:hadPlan dpp:PackingPlan ;
        
dpp:ShippingEvent a prov:Activity ;
    prov:wasAssociatedWith dpp:Supplier2 ;
   
## Prov-O related properties
dpp:generatedFrom a prov:wasGeneratedBy ;
    rdfs:range dpp:SolarPanelFrame .

dpp:activityUsed a rdf:Property ;
	prov:used rdf:type ;
	rdfs:range dpp:SolarPanelFrame .


## Ownership Transfer Event class
dpp:OwnershipTransferEvent a prov:Activity ;
    prov:wasInfluencedBy dpp:SolarPanelFrame ; 
	prov:used rdf:type ;
	rdfs:range dpp:SolarPanelFrame ;
    prov:qualifiedAssociation a prov:Association ;
        prov:hadPlan dpp:OwnershipTransferPlan ;
        prov:hadRole dpp:ContractingParties ;
        prov:hadRole dpp:LegalEntity ;

	## Prov-O related properties for Ownership Transfer
	dpp:wasTransferedFrom a rdf:Property ;
        rdfs:domain dpp:OwnershipTransferEvent ;
	    rdfs:range dpp:Supplier ;
        rdfs:subPropertyOf prov:agent ;
	    
    dpp:wasTransferedTo a rdf:Property ;
        rdfs:domain dpp:OwnershipTransferEvent ;
	    rdfs:range dpp:Manufacturer ;
        rdfs:subPropertyOf prov:agent ;
	       
    dpp:transferDate a rdf:Property ;
        rdfs:domain dpp:OwnershipTransferEvent ;
	    rdfs:range xsd:dateTime ;
        dpp:attribute dpp:Attribute ;
	
    dpp:contractId a rdf:Property ;
        rdfs:domain dpp:OwnershipTransferEvent ;
	    rdfs:range xsd:string ;
        dpp:attribute dpp:Attribute ;